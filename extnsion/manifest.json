{
    "name": "TimeZone Finder",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "popup.js"
      },
    "permissions": [ "activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
          "128": "/images/icon.png"
        }
      },
      "icons": {
        "48": "/icon48.png",
        "128": "/icon.png"
      }
    
}