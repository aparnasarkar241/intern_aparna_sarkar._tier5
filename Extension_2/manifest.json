{
    "name": "SEARCH & PRINT",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
      },
      "permissions": ["storage", "activeTab", "notifications", "scripting"],
      "action": {
        "default_popup": "popup.html",
        "default_icon": {
        
            "128": "/images/search128.png"
          }
      },
      "icons": {
        
        "32": "/images/search32.png",
        "48": "/images/search48.png",
        "128": "/images/search128.png"
      },
      "host_permissions":["*://*.chrome.com/*"],
      "content_scripts":[{
        "css":["style.css"],
        "js":["content.js"],
        "matches":["*://*.chrome.com/*"],
        "run_at":"document_end"
      }]
     
  }